@model Karmyogi.Data.Profile

<div class="grid_3">
    <div class="container">
        <div class="breadcrumb1">
            <ul>
                <a href="@Url.Action("Index","Home")"><i class="fa fa-home home_1"></i></a>
                <span class="divider">&nbsp;|&nbsp;</span>
                <li class="current-page">View Profile</li>
            </ul>
        </div>
        <div class="profile">
            <div class="col-md-8 profile_left">
                <h2>Profile Id : @Model.DisplayID</h2>
                <div class="col_3">
                    <div class="col-sm-4 row_2">
                        <div class="flexslider">
                            <ul class="slides">
                                <li data-thumb="@Url.Content("~/Content/images/p1.jpg")">
                                    <img src="@Url.Content("~/Content/images/p1.jpg")" />
                                </li>
                                <li data-thumb="@Url.Content("~/Content/images/p2.jpg")">
                                    <img src="@Url.Content("~/Content/images/p2.jpg")" />
                                </li>
                                <li data-thumb="@Url.Content("~/Content/images/p3.jpg")">
                                    <img src="@Url.Content("~/Content/images/p3.jpg")" />
                                </li>
                                <li data-thumb="@Url.Content("~/Content/images/p4.jpg")">
                                    <img src="@Url.Content("~/Content/images/p4.jpg")" />
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!---728x90--->
                    <div class="col-sm-8 row_1">
                        <table class="table_working_hours">
                            <tbody>
                                <tr class="opened_1">
                                    <td class="day_label">Age / Height :</td>
                                    <td class="day_value">@Html.AgeFormat(Model.Date_of_Birth.Value), @(Model.Height != null ? Model.Height.HeightText.Replace('-', '/') : "")</td>
                                </tr>
                                <tr class="opened">
                                    <td class="day_label">Last Login :</td>
                                    <td class="day_value">4 hours ago</td>
                                </tr>
                                <tr class="opened">
                                    <td class="day_label">Religion :</td>
                                    <td class="day_value">@(Model.Religion != null ? Model.Religion.ReligionName : "")</td>
                                </tr>
                                <tr class="opened">
                                    <td class="day_label">Marital Status :</td>
                                    <td class="day_value">@Model.Marital_Status</td>
                                </tr>
                                <tr class="opened">
                                    <td class="day_label">Location :</td>
                                    <td class="day_value">@(Model.LiveInCountry != null ? Model.LiveInCountry.Country_nm : "")</td>
                                </tr>
                                <tr class="closed">
                                    <td class="day_label">Profile Created by :</td>
                                    <td class="day_value closed"><span>Self</span></td>
                                </tr>
                                <tr class="closed">
                                    <td class="day_label">Education :</td>
                                    <td class="day_value closed"><span>@(Model.LastDegree != null ? Model.LastDegree.LastDegreeName : "")</span></td>
                                </tr>
                            </tbody>
                        </table>
                        <ul class="login_details">
                            <li>Already a member? <a href="login.html">Login Now</a></li>
                            <li>If not a member? <a href="register.html">Register Now</a></li>
                        </ul>
                    </div>
                    <div class="clearfix"> </div>
                </div>
                <!---728x90--->
                <div class="col_4">
                    <div class="bs-example bs-example-tabs" role="tabpanel" data-example-id="togglable-tabs">
                        <ul id="myTab" class="nav nav-tabs nav-tabs1" role="tablist">
                            <li role="presentation" class="active"><a href="#home" id="home-tab" role="tab" data-toggle="tab" aria-controls="home" aria-expanded="true">About Myself</a></li>
                            <li role="presentation"><a href="#profile" role="tab" id="profile-tab" data-toggle="tab" aria-controls="profile">Family Details</a></li>
                            <li role="presentation"><a href="#profile1" role="tab" id="profile-tab1" data-toggle="tab" aria-controls="profile1">Partner Preference</a></li>
                        </ul>
                        <div id="myTabContent" class="tab-content">
                            <div role="tabpanel" class="tab-pane fade in active" id="home" aria-labelledby="home-tab">
                                <div class="basic_1">
                                    <h3>Basics & Lifestyle</h3>
                                    <div class="col-md-6 basic_1-left">
                                        <table class="table_working_hours">
                                            <tbody>
                                                <tr class="opened_1">
                                                    <td class="day_label">Name :</td>
                                                    <td class="day_value">@Model.F_Name @Model.L_Name</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Marital Status :</td>
                                                    <td class="day_value">@Model.Marital_Status</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Body Type :</td>
                                                    <td class="day_value">@Model.Body_Type</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Marital Status :</td>
                                                    <td class="day_value">@Model.Marital_Status</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Height :</td>
                                                    <td class="day_value">@(Model.Height != null ? Model.Height.HeightText.Replace('-', '/') : "")</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Physical Status :</td>
                                                    <td class="day_value closed"><span>@Model.Physical_Status</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Profile Created by :</td>
                                                    <td class="day_value closed"><span>Self</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Drink :</td>
                                                    <td class="day_value closed"><span>@Model.Drink</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="col-md-6 basic_1-left">
                                        <table class="table_working_hours">
                                            <tbody>
                                                <tr class="opened_1">
                                                    <td class="day_label">Age :</td>
                                                    <td class="day_value">@Html.AgeFormat(Model.Date_of_Birth.Value) Yrs</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Mother Tongue :</td>
                                                    <td class="day_value">@Model.MotherTongue.Language</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Complexion :</td>
                                                    <td class="day_value">@Model.Complexion</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Weight :</td>
                                                    <td class="day_value">@Model.Weight.WeightText</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Blood Group :</td>
                                                    <td class="day_value">@Model.Blood_Group</td>
                                                </tr>
                                                <tr class="closed">
                                                    <td class="day_label">Diet :</td>
                                                    <td class="day_value closed"><span>@Model.Diet</span></td>
                                                </tr>
                                                <tr class="closed">
                                                    <td class="day_label">Smoke :</td>
                                                    <td class="day_value closed"><span>@Model.Smoke</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="clearfix"> </div>
                                </div>
                                <div class="basic_1">
                                    <h3>Religious / Social & Astro Background</h3>
                                    <div class="col-md-6 basic_1-left">
                                        <table class="table_working_hours">
                                            <tbody>
                                                <tr class="opened">
                                                    <td class="day_label">Time of Birth :</td>
                                                    <td class="day_value">@Model.Time_of_Birth</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Caste :</td>
                                                    <td class="day_value">@Model.Caste.CasteName</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Date of Birth :</td>
                                                    <td class="day_value closed"><span>@Model.Date_of_Birth.Value.ToString("dd-MMM-yyyy")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Place of Birth :</td>
                                                    <td class="day_value closed"><span>@Model.Place_of_Birth</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="col-md-6 basic_1-left">
                                        <table class="table_working_hours">
                                            <tbody>
                                                <tr class="opened_1">
                                                    <td class="day_label">Religion :</td>
                                                    <td class="day_value">@Model.Religion.ReligionName</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Sub Caste :</td>
                                                    <td class="day_value">Not Specified</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Raasi :</td>
                                                    <td class="day_value">@Model.Raasi</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="clearfix"> </div>
                                </div>
                                <div class="basic_1 basic_2">
                                    <h3>Education & Career</h3>
                                    <div class="basic_1-left">
                                        <table class="table_working_hours">
                                            <tbody>
                                                <tr class="opened">
                                                    <td class="day_label">Education   :</td>
                                                    <td class="day_value">@Model.LastDegree.LastDegreeName</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Education Detail :</td>
                                                    <td class="day_value">@Model.Profession.ProfessionName</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Occupation Detail :</td>
                                                    <td class="day_value closed"><span>There are many variations of passages of Lorem Ipsum available</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Annual Income :</td>
                                                    <td class="day_value closed"><span>Rs.@Model.Annual_Income</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="clearfix"> </div>
                                </div>
                            </div>
                            <div role="tabpanel" class="tab-pane fade" id="profile" aria-labelledby="profile-tab">
                                <div class="basic_3">
                                    <h4>Family Details</h4>
                                    <div class="basic_1 basic_2">
                                        <h3>Basics</h3>
                                        <div class="col-md-6 basic_1-left">
                                            <table class="table_working_hours">
                                                <tbody>
                                                    <tr class="opened">
                                                        <td class="day_label">Father's Occupation :</td>
                                                        <td class="day_value">@Model.Fathers_Occupation</td>
                                                    </tr>
                                                    <tr class="opened">
                                                        <td class="day_label">Mother's Occupation :</td>
                                                        <td class="day_value">@Model.Mothers_Occupation</td>
                                                    </tr>
                                                    <tr class="opened">
                                                        <td class="day_label">No. of Brothers :</td>
                                                        <td class="day_value closed"><span>@Model.No_of_Brothers</span></td>
                                                    </tr>
                                                    <tr class="opened">
                                                        <td class="day_label">No. of Sisters :</td>
                                                        <td class="day_value closed"><span>@Model.No_of_Sisters</span></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div role="tabpanel" class="tab-pane fade" id="profile1" aria-labelledby="profile-tab1">
                                <div class="basic_1 basic_2">
                                    <div class="basic_1-left">
                                        <table class="table_working_hours">
                                            <tbody>
                                                <tr class="opened">
                                                    <td class="day_label">Age   :</td>
                                                    <td class="day_value">@Model.PartnerPreference.From_Age to @Model.PartnerPreference.To_Age</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Marital Status :</td>
                                                    <td class="day_value">@Model.PartnerPreference.Marital_Status</td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Body Type :</td>
                                                    <td class="day_value closed"><span>@Model.PartnerPreference.Body_Type</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Complexion :</td>
                                                    <td class="day_value closed"><span>@Model.PartnerPreference.Complexion</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Height :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.Height != null ? Model.PartnerPreference.Height.HeightText.Replace('-', '/') : "")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Diet :</td>
                                                    <td class="day_value closed"><span>@Model.PartnerPreference.Diet</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Kujadosham / Manglik :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.IsManglik.Value ? "Yes" : "No")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Religion :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.Religion != null ? Model.PartnerPreference.Religion.ReligionName : "Not Specified")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Caste :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.Caste != null ? Model.PartnerPreference.Caste.CasteName : "Not Specified")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Mother Tongue :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.MotherTongue != null ? Model.PartnerPreference.MotherTongue.Language : "Not Specified")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Education :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.LastDegree != null ? Model.PartnerPreference.LastDegree.LastDegreeName : "Not Specified")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Occupation :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.Profession != null ? Model.PartnerPreference.Profession.ProfessionName : "Not Specified")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Country of Residence :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.Country != null ? Model.PartnerPreference.Country.Country_nm : "Not Specified")</span></td>
                                                </tr>
                                                <tr class="opened">
                                                    <td class="day_label">Residency State :</td>
                                                    <td class="day_value closed"><span>@(Model.PartnerPreference.State != null ? Model.PartnerPreference.State.StateName : "Not Specified")</span></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 profile_right">
                <div class="newsletter">
                    <form>
                        <input type="text" name="ne" size="30" required="" placeholder="Enter Profile ID :">
                        <input type="submit" value="Go">
                    </form>
                </div>
                <div class="view_profile">
                    @Html.Action("ViewSimilarProfiles", "Home",Model)
                </div>
                <div class="view_profile view_profile1">
                    <h3>Members who viewed this profile also viewed</h3>
                    <ul class="profile_item">
                        <a href="#">
                            <li class="profile_item-img">
                                <img src="images/p9.jpg" class="img-responsive" alt="" />
                            </li>
                            <li class="profile_item-desc">
                                <h4>2458741</h4>
                                <p>29 Yrs, 5Ft 5in Christian</p>
                                <h5>View Full Profile</h5>
                            </li>
                            <div class="clearfix"> </div>
                        </a>
                    </ul>
                    <ul class="profile_item">
                        <a href="#">
