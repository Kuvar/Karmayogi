@model Karmyogi.Data.Profile

<div class="profile_top">
    <a href="@Url.Action("ViewProfile","Home",new {@id=Model.SecurityKey })">
        <h2>@Model.DisplayID | Profile Created for self</h2>
        <div class="col-sm-3 profile_left-top">
            <img src="@Url.Content("~/Content/images/a5.jpg")" class="img-responsive" alt="" />
        </div>
        <div class="col-sm-3">
            <ul class="login_details1">
                <li>Last Login : 5 days ago</li>
                <li><p>"Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor." More....</p></li>
            </ul>
        </div>
        <div class="col-sm-6">
            <table class="table_working_hours">
                <tbody>
                    <tr class="opened_1">
                        <td class="day_label1">Age / Height :</td>
                        <td class="day_value"> @Html.AgeFormat(Model.Date_of_Birth.Value), @(Model.Height != null ? Model.Height.HeightText.Replace('-', '/') : "")</td>
                    </tr>
                    <tr class="opened">
                        <td class="day_label1">Last Login :</td>
                        <td class="day_value">4 hours ago</td>
                    </tr>
                    <tr class="opened">
                        <td class="day_label1">Religion :</td>
                        <td class="day_value">@(Model.Religion != null ? Model.Religion.ReligionName : "")</td>
                    </tr>
                    <tr class="opened">
                        <td class="day_label1">Marital Status :</td>
                        <td class="day_value">@Model.Marital_Status</td>
                    </tr>
                    <tr class="opened">
                        <td class="day_label1">Location :</td>
                        <td class="day_value">@(Model.LiveInCountry != null ? Model.LiveInCountry.Country_nm : "") </td>
                    </tr>
                    <tr class="closed">
                        <td class="day_label1">Profile Created by :</td>
                        <td class="day_value closed"><span>Self</span></td>
                    </tr>
                    <tr class="closed">
                        <td class="day_label1">Education :</td>
                        <td class="day_value closed"><span>@(Model.LastDegree != null ? Model.LastDegree.LastDegreeName : "")</span></td>
                    </tr>
                </tbody>
            </table>
            <div class="buttons">
                <div class="vertical">Send Mail</div>
                <div class="vertical">Shortlisted</div>
                <div class="vertical">Send Interest</div>
            </div>
        </div>
        <div class="clearfix"> </div>
    </a>
</div>

